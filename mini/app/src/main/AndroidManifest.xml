    package="com.example.sunita.mini">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Admin_activity"></activity>
        <activity android:name=".Team_activity"></activity>
        <activity android:name=".sign_up_activity"></activity>
        <activity android:name=".database_layout"></activity>
        <activity android:name=".Unsold_player"></activity>
        <activity android:name=".Player_pool"></activity>
        <activity android:name=".Team1"></activity>
        <activity android:name=".Team3"></activity>
        <activity android:name=".Team2"></activity>

    </application>

</manifest>